package main;

import java.util.ListIterator;
import java.util.Scanner;

import clientinfo.Client;

public class MenuStartChooseRole implements IMenuStrategy {
	
	public String menuAction(String command) {
		
		Scanner scanner = new Scanner(System.in);
		
		System.out.println("Are you a client or an employee?");
		String answer = scanner.nextLine();
		System.out.println("You typed: " + answer);
		
		if (scanner.nextLine() == "client") {
			return "client";			
		} else if (scanner.nextLine() == "employee") {
			return "employee";
		} else {
			return "back";
		}

	}

}
